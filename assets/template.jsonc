		"gitlens.graphScrollMarkerHeadColor": "#f904cc", // Scroll marker HEAD
		"gitlens.graphScrollMarkerHighlightsColor": "#e4f403", // Scroll marker highlights
		"gitlens.graphScrollMarkerLocalBranchesColor": "#00ff80", // Scroll marker local branches
		"gitlens.graphScrollMarkerPullRequestsColor": "#ff0080", // Scroll marker pull requests
		"gitlens.graphScrollMarkerRemoteBranchesColor": "#00ffff", // Scroll marker remote branches
		"gitlens.graphScrollMarkerStashesColor": "#ff8000", // Scroll marker stashes
		"gitlens.graphScrollMarkerTagsColor": "#8000ff", // Scroll marker tags

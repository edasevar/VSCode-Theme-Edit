		"icon.foreground": "#ffffff", // Default color for icons throughout the interface
		"sash.hoverBorder": "#ffffff", // Border color for resizable dividers when hovered
		"selection.background": "#404040", // Background color for selected text in input fields
		"widget.border": "#404040", // Border color for widgets and dialog boxes
		"widget.shadow": "#000000", // Shadow color for floating widgets and dialogs

		"gitlens.graphLane3Color": "#8000ff", // Graph lane 3
		"gitlens.graphLane4Color": "#ffff00", // Graph lane 4
		"gitlens.graphLane5Color": "#00ffff", // Graph lane 5
		"gitlens.graphLane6Color": "#ff00ff", // Graph lane 6
		"gitlens.graphLane7Color": "#ff8000", // Graph lane 7
		"gitlens.graphLane8Color": "#00ff00", // Graph lane 8
		"gitlens.graphLane9Color": "#0013ea", // Graph lane 9

		"gitlens.graphMinimapMarkerHighlightsColor": "#e4f403", // Graph minimap highlight marker
		"gitlens.graphMinimapMarkerLocalBranchesColor": "#00ff80", // Graph minimap local branches
		"gitlens.graphMinimapMarkerPullRequestsColor": "#ff0080", // Graph minimap pull requests
		"gitlens.graphMinimapMarkerRemoteBranchesColor": "#00ffff", // Graph minimap remote branches
		"gitlens.graphMinimapMarkerStashesColor": "#ff8000", // Graph minimap stashes
		"gitlens.graphMinimapMarkerTagsColor": "#8000ff", // Graph minimap tags
		"gitlens.graphMinimapMarkerUpstreamColor": "#0013ea", // Graph minimap upstream

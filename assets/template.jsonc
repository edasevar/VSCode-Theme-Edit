		// GLOBAL COLORS & BASIC UI ELEMENTS
		// ========================================
		// Base Colors - Core interface text and element colors
		"descriptionForeground": "#cccccc", // Color for description text in various UI elements
		"disabledForeground": "#808080", // Text color for disabled UI elements
		"errorForeground": "#ff0000", // Global error text color used across the interface
		"focusBorder": "#ffffff", // Border color around focused UI elements
